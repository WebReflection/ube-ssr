{
  "name": "ube-ssr",
  "version": "0.0.0",
  "description": "An SSR, islands friendly, way to render ube components",
  "main": "./cjs/index.js",
  "scripts": {
    "build": "npm run cjs && npm run test",
    "cjs": "ascjs --no-default esm cjs",
    "coveralls": "c8 report --reporter=text-lcov | coveralls",
    "test": "c8 node test/index.js"
  },
  "keywords": [
    "ube",
    "SSR",
    "islands",
    "static",
    "dynamic",
    "site",
    "generator"
  ],
  "author": "Andrea Giammarchi",
  "license": "ISC",
  "devDependencies": {
    "ascjs": "^5.0.1",
    "c8": "^7.7.3",
    "coveralls": "^3.1.0"
  },
  "module": "./esm/index.js",
  "type": "module",
  "exports": {
    ".": {
      "import": "./esm/index.js",
      "default": "./cjs/index.js"
    },
    "./package.json": "./package.json"
  },
  "dependencies": {
    "js-in-json": "^0.1.0",
    "static-params": "^0.1.3",
    "ube": "^0.0.2",
    "uhtml-ssr": "^0.7.1",
    "umeta": "^0.2.4"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/WebReflection/ube-ssr.git"
  },
  "bugs": {
    "url": "https://github.com/WebReflection/ube-ssr/issues"
  },
  "homepage": "https://github.com/WebReflection/ube-ssr#readme"
}
